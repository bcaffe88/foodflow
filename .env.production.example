NODE_ENV=production
PORT=5000

# Database (Railway PostgreSQL)
DATABASE_URL=postgresql://user:password@db.railway.internal:5432/railway

# JWT & Security
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Stripe (Multi-tenant)
STRIPE_SECRET_KEY=sk_live_your_key_here
STRIPE_PUBLIC_KEY=pk_live_your_key_here

# Firebase (Optional - for FCM push notifications)
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_PRIVATE_KEY=your-private-key
FIREBASE_CLIENT_EMAIL=your-email@firebase.com

# SendGrid (Optional - for email notifications)
SENDGRID_API_KEY=SG.your_key_here

# External APIs
GOOGLE_MAPS_API_KEY=your-key-here (optional)
