# Database - PostgreSQL (Replit native or Railway)
DATABASE_URL=postgresql://user:password@localhost:5432/foodflow

# Optional: Railway PostgreSQL fallback
RAILWAY_DATABASE_URL=postgresql://user:password@railway.internal:5432/foodflow

# Supabase Configuration (WhatsApp integration)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# N8N Configuration (Workflow automation)
N8N_API_URL=https://your-n8n-instance.com
N8N_API_KEY=your-n8n-api-key

# Google Maps (Optional - for delivery optimization)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Session & Authentication
SESSION_SECRET=your-session-secret-key
JWT_SECRET=your-jwt-secret-key

# Stripe Configuration
STRIPE_PUBLIC_KEY=pk_test_your-stripe-public-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key

# Node Environment
NODE_ENV=development

# Server Configuration
PORT=5000
HOST=0.0.0.0

# Logging
LOG_LEVEL=debug

# Cache (Redis - Optional)
REDIS_URL=redis://localhost:6379

# Features Flags
ENABLE_WHATSAPP=true
ENABLE_N8N_INTEGRATION=true
ENABLE_DELIVERY_OPTIMIZATION=true
